{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "pages/api/$1",
      "headers": {
        "Cache-Control": "public, s-maxage=900, stale-while-revalidate=60"
      }
    }
  ],
  "functions": {
    "pages/api/create.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "pages/api/[slug].js": {
      "memory": 1024,
      "maxDuration": 15
    }
  },
  "regions": ["iad1"]
}
